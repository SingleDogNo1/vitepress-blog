    "gray-matter": "^4.0.3",
    "highlight.js": "^11.10.0",
    "lodash-es": "^4.17.21",
    "mermaid": "^10.5.0",
    "typed.js": "^2.1.0",
    "vitepress-plugin-mermaid": "2.0.13"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/lodash-es": "^4.17.12",
    "element-plus": "^2.9.0",
