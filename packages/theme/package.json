{
  "name": "@singledog/theme",
  "version": "0.0.0",
  "description": "简约风的 Vitepress 博客主题",
  "main": "src/index.ts",
  "exports": {
    "./node": {
      "types": "./node.d.ts",
      "default": "./node.js"
    },
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "src",
    "types",
    "node.js",
    "node.d.ts"
  ],
  "keywords": [
    "vitepress",
    "theme"
