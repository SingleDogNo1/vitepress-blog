{
  "name": "zcm-blog",
  "version": "0.0.0",
  "private": true,
  "description": "个人博客&知识&文档站集",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^16.18.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "^2.8.1",
    "rimraf": "^5.0.1",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "zx": "^5.1.0"
  },
  "scripts": {
    "reinstall": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm --filter blogpress clean && pnpm --filter @singledog/theme clean && pnpm --filter @singledog/markdown-preview clean && pnpm install",
    "dev": "pnpm --filter blogpress dev",
    "build": "pnpm --filter blogpress build",
    "serve": "pnpm --filter blogpress serve",
    "build:theme": "pnpm --filter @singledog/theme build",
    "build:MDPreview": "pnpm --filter @singledog/markdown-preview build"
  },
  "keywords": [],
